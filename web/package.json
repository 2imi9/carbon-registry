{
  "name": "web",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "antd": "^4.24.1",
    "axios": "^1.1.3",
    "bcrypt": "^5.1.0",
    "env-cmd": "^10.1.0",
    "i18next": "^22.0.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.0.1",
    "jwt-decode": "^3.1.2",
    "luxon": "^3.1.0",
    "node-sass": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-router-dom": "^6.4.3",
    "sha1": "^1.1.1",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start:dev": "PORT=3030 env-cmd -e development react-scripts start",
    "start:prod": "PORT=3030 env-cmd -e production react-scripts start",
    "clean": "npx rimraf build",
    "build": "yarn run clean && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint \"./src/**/*.{ts,tsx,js,jsx,scss,css}\"",
    "lint:fix": "yarn run lint --fix \"./src/**/*.{ts,tsx,js,jsx,scss,css}\"",
    "prettier": "npx prettier --check \"./src/**/*.{ts,tsx,js,jsx,scss,css}\"",
    "prettier:fix": "yarn prettier --write \"./src/**/*.{ts,tsx,js,jsx,scss,css}\"",
    "format": "yarn run prettier:fix && yarn run lint:fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "last 2 versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/sha1": "^1.1.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "react-scripts": "5.0.1",
    "tslint-config-prettier": "^1.18.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
